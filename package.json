{
  "name": "server-vision",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "----   all  ----": "",
    "start:debug": "npm run rm:dist && concurrently \"npm run start:debug:*\"",
    "prisma:generate": "concurrently \"npm run prisma:generate:*\"",
    "prisma:deploy": "concurrently \"npm run migrate:deploy:*\"",
    "build": "concurrently \"npm run build:*\"",
    "rm:dist": "rimraf ./dist",
    "----   gateway  ----": "",
    "start:debug:gateway": "npx dotenv -e ./apps/gateway/.env -- nest start gateway --debug --watch",
    "build:gateway": "nest build gateway",
    "gql:definitions:gateway": "ts-node ./apps/gateway/src/graphql/graphql.definitions.ts",
    "----   accounts  ----": "",
    "start:debug:accounts": "npx dotenv -e ./apps/accounts/.env -- nest start accounts --debug --watch",
    "prisma:generate:accounts": "npx dotenv -e ./apps/accounts/.env -- npx prisma generate --schema ./apps/accounts/src/utils/prisma/account.prisma",
    "migrate:dev:create:accounts": "npx dotenv -e ./apps/accounts/.env -- npx prisma migrate dev --create-only --schema ./apps/accounts/src/utils/prisma/account.prisma",
    "migrate:deploy:accounts": "npx dotenv -e ./apps/accounts/.env -- npx prisma migrate deploy --preview-feature --schema ./apps/accounts/src/utils/prisma/account.prisma",
    "migrate:reset:accounts": "npx dotenv -e ./apps/accounts/.env -- npx prisma migrate reset --preview-feature --schema ./apps/accounts/src/utils/prisma/account.prisma",
    "build:accounts": "nest build accounts"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@nestjs/common": "^10.4.6",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.6",
    "@nestjs/graphql": "^12.2.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mercurius": "^12.2.1",
    "@nestjs/microservices": "^10.4.6",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.6",
    "@nestjs/platform-fastify": "^10.4.6",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.52.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.54.0",
    "@opentelemetry/instrumentation-http": "^0.54.0",
    "@opentelemetry/resources": "^1.27.0",
    "@opentelemetry/sdk-node": "^0.54.0",
    "@opentelemetry/sdk-trace-base": "^1.27.0",
    "@prisma/client": "^5.21.1",
    "@types/joi": "^17.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv-cli": "^7.4.2",
    "graphql": "^16.9.0",
    "graphql-scalars": "^1.23.0",
    "graphql-subscriptions": "^2.0.0",
    "joi": "^17.13.3",
    "mercurius": "^15.1.0",
    "nats": "^2.28.2",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.0",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "ts-morph": "^24.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "concurrently": "^9.0.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.21.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
